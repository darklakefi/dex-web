{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "grpc-client",
  "projectType": "library",
  "sourceRoot": "libs/grpc-client/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/grpc-client",
        "command": "tsc --build"
      },
      "cache": true,
      "inputs": [
        "production",
        "^production",
        {
          "externalDependencies": ["typescript"]
        }
      ],
      "outputs": ["{workspaceRoot}/dist/libs/grpc-client"]
    }
  }
}
