{
  "compilerOptions": {
    "allowJs": true,
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "es5"
  },
  "exclude": ["node_modules", ".next"],
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [
    ".next/types/**/*.ts",
    "../../types/**/*.d.ts",
    "../../types/svg.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
